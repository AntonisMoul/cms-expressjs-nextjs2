{
  "name": "cms-expressjs-nextjs2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter './apps/*' dev",
    "dev:api": "pnpm --filter '@cms/api' dev",
    "dev:web": "pnpm --filter '@cms/web' dev",
    "worker": "pnpm --filter '@cms/api' worker",
    "scheduler": "pnpm --filter '@cms/api' scheduler",
    "build": "pnpm --recursive build",
    "db:migrate": "pnpm --filter '@cms/shared' db:migrate",
    "db:generate": "pnpm --filter '@cms/shared' db:generate",
    "db:studio": "pnpm --filter '@cms/shared' db:studio",
    "setup:local": "pnpm db:migrate && pnpm --filter '@cms/shared' seed"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prisma": "^5.22.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
